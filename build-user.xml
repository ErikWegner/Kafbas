<target name="help">
    <echo message="Please run: $ ant -v -projecthelp"/>
</target>
<target name="dailyrelease" description="Erstellt eine tagesaktuelle JAR-Datei" depends="build">
	<tstamp />
    <jar destfile="${DSTAMP}-kafbas.jar" >
		<manifest>
			<attribute name="Build-By" value="Erik Wegner"/>
			<attribute name="Main-class" value="de.ewus.kafbas.Kafbas"/>
		</manifest>
		<fileset dir="bin">
			<include name="**"/>
		</fileset>
	</jar>
</target>

